<h2><%= @list.name %></h2>
<div>
  <ul>
    <% @list.bookmarks.each do |bookmark| %>
      <li><br><%= bookmark.movie %>
      <br><%= bookmark.comment %></li>
      <% end %>
  </ul>
</div>
<div>
  <%= link_to "New bookmark", new_list_bookmark_path(@list) %>
</div>

<div>
  <%= link_to "delete list",
    list_path(@list),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
    %>
</div>
